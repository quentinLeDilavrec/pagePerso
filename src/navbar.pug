nav.my-nav(style="position: absolute;border-radius: 10px;background: #73AD21;padding: 10px; width: 50px;height: 60px;right: 0%;margin:20px;margin-top:3px; z-index: 100")
  a.trigger(href="#0", onClick="this.nextSibling.nextSibling.style.visibility = 'visible';") Menu
    span(aria-hidden="true")
  ul(style="visibility:hidden;")
    each p in pages
      li(visibility='hidden')
        a(href='#') #{p}